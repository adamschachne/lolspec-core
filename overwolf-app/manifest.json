{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"LoL Spectator",
    "version":"1.0.0.0",
    "minimum-overwolf-version": "0.85.0.0",
    "author":"Overwolf",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"spectate League of legends"
  },
  "permissions": ["Extensions", "GameInfo"],
  "data": {
    "start_window":"index",
    "windows":{
      "index":{
        "file":"index.html",
        "transparent":false,
        "resizable":true,
        "show_in_taskbar": true,
		"in_game_only": false,
        "size": {
          "width": 400,
          "height": 400
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    },
	"game_events": [5426],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [5426]
        },
        "start_minimized": false
      }
    ],
	"extra-objects": {
		"testplugin": {
          "file": "test-plugin.dll",
          "class": "test_plugin.EntryPoint"
        }
    }
  }
}
